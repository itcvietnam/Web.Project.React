{
  "palette": {
    "primary": {
      "main": "#0062af",
      "contrastText": "#ffffff"
    }
  },
  "shape": {
    "borderRadius": 18
  },
  "components": {
    "MuiButton": {
      "defaultProps": {
        "fullWidth": true,
        "size": "large",
        "disableRipple": true,
        "disableElevation": true
      }
    },
    "MuiSelect": {
      "defaultProps": {
        "inputProps": {
          "IconComponent": false
        }
      },
      "styleOverrides": {
        "root": {}
      }
    },
    "MuiTextField": {
      "defaultProps": {
        "inputProps": {},
        "InputProps": {
          "disableUnderline": true
        },
        "variant": "filled"
      },
      "styleOverrides": {
        "root": {}
      }
    },
    "MuiInputBase": {
      "styleOverrides": {
        "root": {
          "borderRadius": "18px"
        }
      }
    },
    "MuiFilledInput": {
      "styleOverrides": {
        "input": {
          "padding": "10px",
          "fontWeight": "bold"
        }
      }
    }
  }
}